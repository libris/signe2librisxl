prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>
prefix owl: <http://www.w3.org/2002/07/owl#>
prefix xsd: <http://www.w3.org/2001/XMLSchema#>
prefix skos: <http://www.w3.org/2004/02/skos/core#>
prefix prov: <http://www.w3.org/ns/prov#>
prefix sdo: <http://schema.org/>
prefix ptg: <http://protege.stanford.edu/plugins/owl/protege#>
prefix dc: <http://purl.org/dc/terms/>
prefix quid: <https://w3id.org/quid/>
prefix : <https://id.kb.se/vocab/>

# NEW:

:TemporalEntity a owl:Class ;
    ptg:abstract true ;
    rdfs:subClassOf quid:QualifiedIdentity .

:languagePeriod a owl:ObjectProperty ;
    #rdfs:domain :Serial ;
    rdfs:range :LanguagePeriod .

:LanguagePeriod a owl:Class ;
    rdfs:subClassOf :TemporalEntity .

:politicalTendencyPeriod a owl:ObjectProperty ;
    #rdfs:domain :Serial ;
    rdfs:range :PoliticalTendencyPeriod .

:PoliticalTendencyPeriod a owl:Class ;
    rdfs:subClassOf :TemporalEntity .

:politicalTendency a owl:ObjectProperty ;
    skos:related :intendedAudience ;
    sdo:domainIncludes :PoliticalTendencyPeriod ;
    rdfs:range :PoliticalTendency .

:PoliticalTendency a owl:Class ;
    rdfs:subClassOf :Concept .

:firstIssueDate a owl:DatatypeProperty ;
    rdfs:subPropertyOf :firstIssue, :startDate ;
    sdo:domainIncludes :FormerTitle, :TemporalEntity ;
    rdfs:range xsd:date .

:lastIssueDate a owl:DatatypeProperty ;
    rdfs:subPropertyOf :lastIssue, :endDate ;
    sdo:domainIncludes :FormerTitle, :TemporalEntity ;
    rdfs:range xsd:date .

:frequencyPeriod rdfs:subPropertyOf :pubFrequency .
:FrequencyPeriod rdfs:subClassOf :PubFrequency, :TemporalEntity .

:TemporalCoverage a owl:Class ;
    rdfs:subClassOf :TemporalEntity .

# TODO: Establish this notion (it is a "group in time" for editions)
:SerialEdition a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty :issuanceType ;
            owl:hasValue :Serial ] .

:isIssueOfEdition a owl:ObjectProperty ;
    rdfs:subPropertyOf :isPartOf ;
    rdfs:domain :Instance ; # :Serial
    rdfs:range :SerialEdition .

# (map monographies' editionStatement "<signe-id>" to this...)
:editionStatement owl:propertyChainAxiom (:isIssueOfEdition :label) .

:generatedEdition a owl:ObjectProperty ;
    rdfs:subPropertyOf prov:generated ;
    sdo:domainIncludes :Manufacture ;
    rdfs:range :SerialEdition .

# MODIFIED:

:pubFrequency :category :pending .
:PubFrequency :category :pending .

:temporalCoverage a owl:ObjectProperty ;
    rdfs:domain :Work ;
    rdf:range :TemporalCoverage .

:geographicCoverage rdfs:subPropertyOf dc:spatial .
